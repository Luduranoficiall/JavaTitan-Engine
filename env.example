# Exemplo sem segredos (ajuste conforme o ambiente)

# APP
JAVATITAN_PORT="8080"
JAVATITAN_HTTP_THREADS="8"
JAVATITAN_WORKER_THREADS="4"
JAVATITAN_SIMULATED_DELAY_MS="0"
JAVATITAN_ALLOW_PLAIN="false"
JAVATITAN_MAX_BODY_KB="64"
JAVATITAN_PROCESS_TIMEOUT_MS="12000"
JAVATITAN_RATE_LIMIT_PER_MIN="120"
JAVATITAN_METRICS_ENABLED="true"

# JWT
JAVATITAN_JWT_SECRET="<PREENCHER>"
JAVATITAN_JWT_REQUIRE_EXP="true"
JAVATITAN_JWT_ISS=""
JAVATITAN_JWT_AUD=""
JAVATITAN_JWT_CLOCK_SKEW="30"
JAVATITAN_JWT_TTL="3600"
JAVATITAN_JWT_PLAN="PRO"

# TLS
JAVATITAN_TLS_KEYSTORE_PATH="<PREENCHER>"
JAVATITAN_TLS_KEYSTORE_PASSWORD="<PREENCHER>"
JAVATITAN_TLS_KEYSTORE_TYPE="JKS"
JAVATITAN_TLS_KEY_PASSWORD=""

# mTLS
JAVATITAN_TLS_REQUIRE_CLIENT_AUTH="true"
JAVATITAN_TLS_TRUSTSTORE_PATH="<PREENCHER>"
JAVATITAN_TLS_TRUSTSTORE_PASSWORD="<PREENCHER>"
JAVATITAN_TLS_TRUSTSTORE_TYPE="JKS"

# TLS cliente (para TestClient/TccSmokeTest)
JAVATITAN_CLIENT_KEYSTORE_PATH="<PREENCHER>"
JAVATITAN_CLIENT_KEYSTORE_PASSWORD="<PREENCHER>"
JAVATITAN_CLIENT_KEYSTORE_TYPE="JKS"
JAVATITAN_CLIENT_KEY_PASSWORD=""
JAVATITAN_CLIENT_TRUSTSTORE_PATH="<PREENCHER>"
JAVATITAN_CLIENT_TRUSTSTORE_PASSWORD="<PREENCHER>"
JAVATITAN_CLIENT_TRUSTSTORE_TYPE="JKS"

# AES (payload)
JAVATITAN_SECURE_MODE="true"
JAVATITAN_AES_KEY="<PREENCHER>"

# DB
JAVATITAN_DB_URL=""
JAVATITAN_DB_USER=""
JAVATITAN_DB_PASS=""
JAVATITAN_DB_DRIVER=""

# Smoke / relatorios
JAVATITAN_SMOKE_REPORT_PATH="reports/tcc-smoke.json"
JAVATITAN_SMOKE_REPORT_CSV="reports/tcc-smoke.csv"
JAVATITAN_SMOKE_CHECK_METRICS="false"
JAVATITAN_BASE_URL="https://localhost:8080"
JAVATITAN_JWT_TOKEN=""
